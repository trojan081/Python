# Fibonacci numbers
A = int(input())
f1 = 0
f2 = 1
if A in (0, 1):
    print (A)
else:
    index = 1
    while True:
        fn = f1 + f2
        f1 = f2 
        f2 = fn
        index += 1
        if fn < A:
            pass
        elif A == fn:
            print (index)
            break
        else:
            print (-1)
            break
            
            
