IP = ((input()))
num1 = ''
num2 = ''
num3 = ''
num4 = ''
count = 0
n = len(IP)
for i in range(n):
    if IP[i] == '.':
        count += 1
        break
    else:
        if int(IP[i]) in range (0, 10):
            num1 += IP[i]
            count += 1
for i in range(count, n):
    if IP[i] == '.':
        count += 1
        break
    else:
        if int(IP[i]) in range (0, 10):
            num2 += IP[i]
            count += 1
for i in range(count, n):
    if IP[i] == '.':
        count += 1
        break
    else:
        if int(IP[i]) in range (0, 10):
            num3 += IP[i]
            count += 1
for i in range(count, n):
    if IP[i] == '.':
        num4 = '404'
        break
    else:
        if int(IP[i]) in range (0, 10):
            num4 += IP[i]
            count += 1
if int(num1) in range (0, 256) and int(num2) in range (0, 256) and int(num3) in range (0, 256) and int(num4) in range (0, 256):
    print ('YES')
else:
    print ('NO')
