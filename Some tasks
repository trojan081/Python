IP = ((input()))
num1 = ''
num2 = ''
num3 = ''
num4 = ''
for i in range(16):
    if int(IP[i]) in range (0, 9):
        num1 += IP[i]
    else:
        if int(num1) not in range (0, 256):
            print ('NO')
            break
        else:
            for f in range (i+1, 16):
                if int(IP[f]) in range (0, 9):
                    num2 += IP[f]
                else:
                    if int(num2) not in range (0, 256):
                        print ('NO')
                        break
                    else:
                        for e in range (f+1, 16):
                            if int(IP[e]) in range (0, 9):
                                num3 += IP[e]
                            else:
                                if int(num3) not in range (0, 256):
                                    print ('NO')
                                    break
                                else:
                                    for d in range (e+1, 16):
                                        if int(IP[d]) in range (0, 9):
                                            num4 += IP[d]
                                        else:
                                            if int(num3) not in range (0, 256):
                                                print ('NO')
                                                break
if int(num1, num2, num3, num4) in range (0, 256):
    print ('YES')
    
